    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "build-ios": "nx build --configuration=--production && nx run appacut-app:sync:ios && nx run appacut-app:open:ios",
    "trapeze-run": "npx trapeze run apps/appacut-app/config.yaml --android-project apps/appacut-app/android --ios-project apps/appacut-app/ios/App",
    "postinstall": "ngcc --properties es2020 browser module main",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:build": "nx affected:test",
    "affected:test": "nx affected:test",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "migrate": "ts-node --project apps/api/tsconfig.migrations.json apps/api/migrate.ts",
    "install-resources-ios": "cd apps/appacut-app/src && cordova-res ios --skip-config --copy --ios-project ../../../ios",
    "install-resources-android": "echo 'This will successfully install the splash screen but will overwrite icons, you will need to re-install icons manually after running this command, please see Readme.md' && cd apps/appacut-app/src && cordova-res android --skip-config --copy --android-project ../android"
